{
  "name": "Consumer",
  "dockerComposeFile": "../../docker-compose.yml",
  "service": "consumer",
  "runServices": [
    "consumer",
    "postgres" //need to run also postgres to use the db
  ],
  "workspaceFolder": "/app",
  "overrideCommand": true,
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.black-formatter", "ritwickdey.liveserver","ms-python.debugpy"]
    }
  },
  "postCreateCommand": "git config --global user.name \"$GIT_USER\" && git config --global user.email \"$GIT_EMAIL\""
}
